pipeline {
    agent any
    
    tools{
        maven "Maven-3.9.4"
    }

    stages {
        stage('Clone') {
            steps {
                git https://github.com/Ekangaki/maven-web-app3.git
            }
        }
         stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
    }
}
